Option Explicit

  Public gintPrivBelegArt As Integer

  Private objPRM As clsPRM
  Private objTDBG As clsTDBG7ole
  Private objHlp As SpHlp.clsHlp
  Private Datumausw As New SPDatumAusw.clsDatumAusw
  
  Private WithEvents gRS  As ADODB.Recordset
Attribute gRS.VB_VarHelpID = -1
  Private Col As TrueOleDBGrid70.Column
  Private cols As TrueOleDBGrid70.Columns
  Private gIntSortColIndex As Integer
  Private gstrSortOrder As String
  'LL
  Private glRet As Long

Public Function LlPrint(Mode As Integer, Optional Save As Boolean) As Long
  'Mode = 2 -> Vorschau
  'Mode = 1 -> Druck
  'Save = True -> LL-Datei wird gesichert.
  'Die Option wird genutzt um die Belege zu archivieren. (Im 2-ten Durchlauf nachdem die Belege gedruckt worden.)
  
  Dim Formular As String
  Dim i As Integer
  Dim Msg As Boolean
  Dim MsgBoxText As String
  Dim SperrText As String
  Dim NrKreisText As String
  Dim DruckIni As Boolean
  Dim rs As DAO.Recordset
  Dim RS1 As DAO.Recordset
  Dim ZwSumme As Double
  Dim SQL As String
  
  Dim SteuerPfl As Double
  Dim SteuerFr As Double
  Dim USt As Double
  Dim Betrag As Double
  Dim SteuerPflWrg As Double
  Dim SteuerFrWrg As Double
  Dim UStWrg As Double
  Dim BetragWrg As Double
  Dim Kurs As Double
  
  Dim BelegArt As Integer
  Dim BelegDatum As Variant
  Dim BelegID As Long
  Dim Waehrung As String
  Dim Skonto As Single
  Dim SkontoTage As Integer
  Dim NettoTage As Integer
  Dim MwSt As Single
  Dim RechnNr As Long
 
  Dim Seite As Long
  
On Error GoTo Fehler
  
  If gRS.RecordCount > 0 Then
    
    gRS.MoveFirst
    
    Screen.MousePointer = 11
    
    Do Until gRS.EOF
      If gRS!Status = -1 Then
        Msg = False
        BelegID = gRS!BelegID
        Set rs = GDB.OpenRecordset("SELECT * FROM [2800_Haupt] WHERE BelegID = " & BelegID, dbOpenDynaset, dbSeeChanges)
       
        If rs.RecordCount > 0 Then
          If Mode < 2 Then
            'Überprüfen, ob in der Zwischenzeit von einer andern Arbeitsstation der Beleg gedruckt wurde.
            If rs!Druck = 0 Then
              
              GWS.BeginTrans
              If rs!BelegNr = 0 Then
                'Neue Rechnung
                
                Do
                  RechnNr = NummernKreis(NummernKreisWaehlen(gintPrivBelegArt + 8))  'Prüfen, ob allgemainer Nummernkreis für Rechnungen benutzt werden soll.
                  'Wenn die ermittelte RechnNr bereits existiert, neue RechnNr ermitteln.
                  If RechnNr = -1 Then Exit Do
                Loop Until IstBelegNrFrei(RechnNr, BelegID, gintPrivBelegArt)
                
                'RechnNr = NummernKreis(NummernKreisWaehlen(gintPrivBelegArt + 8), False, False) 'Prüfen, ob allgemainer Nummernkreis für Rechnungen benutzt werden soll.
                If RechnNr = -1 Then
                  'Fehler beim Ziehen der Rechnungsnummer.
                  If NrKreisText = "" Then
                    NrKreisText = "Folgende Belege wurden nicht gedruckt da der Nummernkreis von einem anderen Benutzer gesperrt war:"
                    NrKreisText = NrKreisText & vbCrLf & "Erf.-Nr: - " & rs!BelegID & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
                  Else
                    NrKreisText = NrKreisText & vbCrLf & "Erf.-Nr: - " & rs!BelegID & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
                  End If
                  GWS.Rollback
                  GoTo NextRecord

                Else
                  rs.Edit
                  rs!BelegNr = RechnNr
                  rs!Druck = 1
                  rs!AendDat = Now
                  rs!AendVon = GsUser
                  If IsNull(rs!BelegDatum) Then
                    rs!BelegDatum = GdtDatum
                  End If
                  rs.Update
                End If
              Else
                'rs!BelegNr <> 0
                rs.Edit
                rs!Druck = 1
                rs!AendDat = Now
                rs!AendVon = GsUser
                If IsNull(rs!BelegDatum) Then
                  rs!BelegDatum = GdtDatum
                End If
                rs.Update
              End If
              
              SpeditionsBuch rs!BelegID, rs!Art
              
              GWS.CommitTrans
              
            Else
              Msg = True
              If MsgBoxText = "" Then
                MsgBoxText = "Folgende Belege wurden von anderen Benutzern gedruckt:"
                MsgBoxText = MsgBoxText & vbCrLf & rs!BelegNr & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
              Else
                MsgBoxText = MsgBoxText & vbCrLf & rs!BelegNr & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
              End If
            End If
          End If
          
          If Msg = False Then
            LL1.LlDefineVariableStart 'Variablenpuffer löschen.
            LL1.LlDefineFieldStart 'Variablenpuffer löschen.
            
            Call LLGestaltungFormular(LL1, rs!Art + 1)
            Call LLDefineVariablen(LL1, rs, "Kd_")
            Call LLDefineFelder(LL1, rs, "Kd_")
            
            BelegArt = rs!Art
            BelegDatum = rs!BelegDatum
            Waehrung = rs!Wrg1
            Skonto = rs!ZSkto
            SkontoTage = rs!ZSktoTage
            NettoTage = rs!ZTage
            MwSt = rs!MwSt
            Kurs = rs!Kurs
            rs.Close
                
            'Folge-Recordset
            Set rs = GDB.OpenRecordset("SELECT * FROM [2800_Folge] WHERE BelegID = " & BelegID & " ORDER BY Nr", dbOpenSnapshot)
            
            If rs.RecordCount > 0 Then
              Call LLDefineVariablen(LL1, rs, "Re_")
              ZwSumme = 0
              Seite = 1
              LL1.LlDefineVariableExt "Seite", Seite, LL_NUMERIC
              LL1.LlDefineFieldExt "Re_ZwSumme", 0, LL_NUMERIC
              LL1.LlDefineFieldExt "ZahlungsZiel", "", LL_TEXT
              LL1.LlDefineFieldExt "LetzteSeite", 0, LL_NUMERIC
              
              'Betrag für ZahlungsZiel
              SQL = "SELECT Sum(IIf([Steuer]=1,([Menge]*[EPreis]/IIf([Einheit]='%',100,1))-([Menge]*[EPreis]/IIf([Einheit]='%',100,1)*[Rabatt]/100),0)) AS SteuerPfl, "
              SQL = SQL & "Sum(IIf([Steuer]=0,([Menge]*[EPreis]/IIf([Einheit]='%',100,1))-([Menge]*[EPreis]/IIf([Einheit]='%',100,1)*[Rabatt]/100),0)) AS SteuerFr "
              SQL = SQL & "FROM [2800_Folge] WHERE BelegID = " & BelegID
              SQL = SQL & " HAVING SatzTyp='A' OR SatzTyp='P'"
              Set RS1 = GDB.OpenRecordset(SQL, dbOpenSnapshot)
              
              SteuerPfl = Runden(RS1!SteuerPfl, 2)
              SteuerFr = Runden(RS1!SteuerFr, 2)
              USt = Runden((SteuerPfl * MwSt / 100), 2)
              Betrag = SteuerPfl + USt + SteuerFr
              
              SteuerPflWrg = Runden(SteuerPfl * Kurs, 2)
              SteuerFrWrg = Runden(SteuerFr * Kurs, 2)
              UStWrg = Runden(USt * Kurs, 2)
              BetragWrg = SteuerPflWrg + UStWrg + SteuerFrWrg
              
              LL1.LlDefineFieldExt "Re_SummeSteuerPfl", SteuerPfl, LL_NUMERIC
              LL1.LlDefineFieldExt "Re_SummeSteuerFr", SteuerFr, LL_NUMERIC
              LL1.LlDefineFieldExt "Re_USt", USt, LL_NUMERIC
              LL1.LlDefineFieldExt "Re_Betrag", Betrag, LL_NUMERIC
              
              LL1.LlDefineFieldExt "Re_SummeSteuerPflWrg", SteuerPflWrg, LL_NUMERIC
              LL1.LlDefineFieldExt "Re_SummeSteuerFrWrg", SteuerFrWrg, LL_NUMERIC
              LL1.LlDefineFieldExt "Re_UStWrg", UStWrg, LL_NUMERIC
              LL1.LlDefineFieldExt "Re_BetragWrg", BetragWrg, LL_NUMERIC
              
              'Soll Spalte Rabatt sichtbar sein?
              SQL = "SELECT Max([Rabatt]) AS MaxRabatt "
              SQL = SQL & "FROM [2800_Folge] WHERE BelegID = " & BelegID
              Set RS1 = GDB.OpenRecordset(SQL, dbOpenSnapshot)
              LL1.LlDefineFieldExt "RabattVisible", RS1!MaxRabatt, LL_NUMERIC
              'Damit rs.RecordCount abgefragt werden kann.
              rs.MoveLast
              rs.MoveFirst
            Else
              Msg = True
            End If
          End If
        Else
          Msg = True
        End If
        
        If Msg = False Then
          If DruckIni = False Then
            'Der Drucker wird nur beim ersten Durchlauf initialisiert.
            Formular = FormularPfad("SP52800.lst")
            
            glRet = LL1.LlPreviewDeleteFiles(Formular, "")
            
            If Mode < 2 Then 'Druck
              glRet = LL1.LlPrintWithBoxStart(LL_PROJECT_LIST, Formular, LL_PRINT_NORMAL, LL_BOXTYPE_BRIDGEMETER, Me.hwnd, "printing list")
            Else 'Vorschau
              If Save Then
                glRet = LL1.LlPrintStart(LL_PROJECT_LIST, Formular, LL_PRINT_PREVIEW)
              Else
                glRet = LL1.LlPrintWithBoxStart(LL_PROJECT_LIST, Formular, LL_PRINT_PREVIEW, LL_BOXTYPE_BRIDGEMETER, Me.hwnd, "printing list to preview")
              End If
            End If
            If glRet < 0 Then
              GoTo Fehler
            End If
            If CBool(GetSetting("SP50000", "SP52800", "SP52850DruckerDialog", "-1")) = True Then 'Druckdialog
              If Not Save Then glRet = LL1.LlPrintOptionsDialog(Me.hwnd, "Drucker")
            End If
            
            If glRet = LL_ERR_USER_ABORTED Then
              Screen.MousePointer = 0
              Exit Function
            End If
            
            Screen.MousePointer = 11
            DruckIni = True
          End If
            
          'Variablen drucken
          LL1.LlPrint
          
        
          'Solange das Ende der Posten-Tabelle nicht erreicht ist...
          While Not rs.EOF
            DoEvents
            'Prozentbalken setzen
            glRet = LL1.LlPrintSetBoxText("Drucken", (100# * gRS.AbsolutePosition / gRS.RecordCount))
            'Datensatzfelder der Liste bekanntmachen.
            If Trim(rs!Einheit) = "%" Then
              ZwSumme = ZwSumme + Runden((rs!Menge / 100 * rs!Epreis - rs!Menge * rs!Epreis * rs!Rabatt / 100), 2)
            Else
              ZwSumme = ZwSumme + Runden((rs!Menge * rs!Epreis - rs!Menge * rs!Epreis * rs!Rabatt / 100), 2)
            End If
            LL1.LlDefineFieldExt "Re_ZwSumme", ZwSumme, LL_NUMERIC
            
            If rs.RecordCount = rs.AbsolutePosition + 1 Then
              LL1.LlDefineFieldExt "LetzteSeite", 1, LL_NUMERIC
              If gintPrivBelegArt = 0 Then 'Nur bei Rechnungen
                LL1.LlDefineFieldExt "ZahlungsZiel", ZahlungsZiel(BelegDatum, Betrag, Waehrung, Skonto, SkontoTage, NettoTage), LL_TEXT
              End If
            End If
            
            Call LLDefineFelder(LL1, rs, "Re_")
            
            'Seitenumbruch
            If rs!SatzTyp = "S" Then
              Seite = Seite + 1
              LL1.LlDefineVariableExt "Seite", Seite, LL_NUMERIC
              glRet = LL1.LlPrint
            End If
            
            'Felder drucken und wenn Seitenumbruch erfolgt ist,
            'Variablen und Felder erneut drucken
            While LL1.LlPrintFields = LL_WRN_REPEAT_DATA
              Seite = Seite + 1
              LL1.LlDefineVariableExt "Seite", Seite, LL_NUMERIC
              glRet = LL1.LlPrint
            Wend
            If rs!SatzTyp = "Z" Then ZwSumme = 0
            rs.MoveNext
          Wend
        End If
        
      End If
      
NextRecord:
      gRS.MoveNext
    Loop
      
      
    If DruckIni Then
      gRS.MoveFirst
      Screen.MousePointer = 0
      'Tabellen-Ausdruck beenden
      Do
        glRet = LL1.LlPrintFieldsEnd()
      Loop Until glRet <> LL_WRN_REPEAT_DATA
    
      'Wenn der Benutzer abbricht
      If glRet = LL_ERR_USER_ABORTED Then
        LL1.LlPrintEnd (0)
        Exit Function
      End If
        
      'Druck beenden
      glRet = LL1.LlPrintEnd(0)
     
      'Beim Preview-Druck Preview anzeigen und dann Preview-Datei (.LL) löschen
      If Mode = 2 Then 'PrintMode = LL_PRINT_PREVIEW
        If Save Then
          If BelegArt = 0 Then
            Archivieren "SFR"
          Else
            Archivieren "SFG"
          End If
        Else
          glRet = LL1.LlPreviewDisplay(ArbeitsplatzPfad & "\SP52800.LL", "", Me.hwnd)
        End If
        glRet = LL1.LlPreviewDeleteFiles(ArbeitsplatzPfad & "\SP52800.LL", "")
      End If
      
    End If
    
    'GWS.Rollback
  End If
  
  Screen.MousePointer = 0
  
  If MsgBoxText <> "" Or SperrText <> "" Or NrKreisText <> "" Then
    If MsgBoxText <> "" Then
      If SperrText <> "" Then
        MsgBoxText = MsgBoxText & vbCrLf & vbCrLf & SperrText
      End If
    Else
      If SperrText <> "" Then
        MsgBoxText = SperrText
      End If
    End If
    
    If MsgBoxText <> "" Then
      If NrKreisText <> "" Then
        MsgBoxText = MsgBoxText & vbCrLf & vbCrLf & NrKreisText
      End If
    Else
      If NrKreisText <> "" Then
        MsgBoxText = NrKreisText
      End If
    End If
    
    MsgBox MsgBoxText, vbInformation
  End If
  
  
        Exit Function
Fehler:
        If IsUpdateError(Err.Number) Then
          If SperrText = "" Then
            SperrText = "Folgende Belege wurden nicht gedruckt da sie von anderen Benutzern bearbeitet waren:"
            SperrText = SperrText & vbCrLf & rs!BelegNr & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
          Else
            SperrText = SperrText & vbCrLf & rs!BelegNr & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
          End If
          GWS.Rollback
          GoTo NextRecord
        Else
          gRS.MoveFirst
          Screen.MousePointer = 0
          If glRet <> 0 Then
            Call FehlerErklärung("SP52800B", "LLPrint LL-Fehler: " & glRet)
          Else
            Call FehlerErklärung("SP52800B", "LLPrint")
          End If
          'Beim Drucken ist ein fehler aufgeteten.
          'Die Änderungen in der DB müssen rückgängig gemacht werden.
          'evtl. RollBack
          glRet = LL_ERR_USER_ABORTED '-99
        
        End If
        
End Function






Public Function AusDBLesen() As Boolean
               '***Beginn
On Error GoTo Fehler
               '***Ende
               
  Dim rs As New ADODB.Recordset
  Dim SQL As String
  Dim cn As New ADODB.Connection
  Dim Datei As String
  Dim exist As Boolean
  Dim PanelText As String
  Dim Felder As String
  
  cn.Mode = adModeUnknown
  cn.Provider = "Microsoft.Jet.OLEDB.4.0"
  
  'Isolation und Kapselung der Transaktionen zum Herabsetzen des Transaktionslevels
  cn.IsolationLevel = adXactCursorStability
  
  cn.CursorLocation = adUseServer
  
  Felder = objPRM.GetUseFields("TDBG1")
  If left(Felder, 7) = "Status," Then
    'Das Feld Status wird erst später eingefügt.
    Felder = right(Felder, Len(Felder) - 7)
  End If
  
  SQL = "SELECT " & Felder & " FROM [2800_Haupt] WHERE Storno = '0' AND Druck = 0 AND Art = " & gintPrivBelegArt
  
  cn.Open GsHauptPfad + "dat\" & CStr(CInt(GsAnwenderNr)) & "\SP50000.dat"
  Set rs.ActiveConnection = cn
  rs.CursorLocation = adUseServer
  'rs.CursorType = adOpenStatic
  rs.CursorType = adOpenKeyset
  rs.Open SQL
  
  If Not gRS Is Nothing Then
    gRS.Close
    Set gRS = Nothing
  End If

  Set gRS = New ADODB.Recordset
  Set gRS = RsOhneVerbindung(rs, "Status") 'Das Feld Status wurde an den RS angehängt.
  gRS.Sort = "Name1"
  
  Set TDBG1.DataSource = gRS
 
               '***Beginn
        Exit Function
Fehler:
        Call FehlerErklärung("frmSP52832", "AusDBLesen")
               '***Ende

End Function


