Private Sub txt1_GotFocus(Index As Integer)
 '***Beginn
On Error GoTo Fehler
 '***Ende
  txt1(Index).SelStart = 0
  txt1(Index).SelLength = Len(txt1(Index))
  txt1(Index).ForeColor = vbWindowText
  txt1(Index).BackColor = &HC0E0FF  'hellorange
  objPRM.FindFirstString = "name = 'txt1' AND index = " & Index
  'txt1(Index).MaxLength = objPRM.EingabeLaenge(txt1(Index))

  
  
    
 '***Beginn
        Exit Sub
Fehler:
        Call FehlerErklärung("frmSP52850", "txt1_GotFocus")
 '***Ende

End Sub


Private Sub txt1_KeyDown(Index As Integer, KeyCode As Integer, Shift As Integer)
 '***Beginn
On Error GoTo Fehler
 '***Ende
  
  Select Case KeyCode
  Case vbKeyF1
    If Shift = 1 Then
      'Die UMSCHALT-TASTE ist gedrückt.
      'Hilfetexte können erfast oder bearbeitet werden.
      objHlp.HlpShow HlpWrite, objPRM.HlpID
    Else
      objHlp.HlpShow HlpRead, objPRM.HlpID
    End If
  Case vbKeyF2
    Select Case Index
    Case 0, 1
      Call cmdAuswahl_Click(Index)
    End Select
  Case vbKeyReturn, vbKeyDown
    objPRM.FindFirstString = "name = 'txt1' AND index = " & Index
    'Weil objPRM.SprungNeu Validate-Ereignis nicht auslöst,
    'muss die Umwandlung und Prüfung an der Stelle stattfinden.
    txt1(Index) = objPRM.EingabeUmwandlung(txt1(Index), , False, True)
    If objPRM.EingabeFehler(txt1(Index)) = False Then
      gRS.Filter = getFilter()
      Call objPRM.SprungNeu("Vorwärts", Shift, txt1(Index).TabIndex, True)
    End If
  Case vbKeyEscape, vbKeyUp
    objPRM.FindFirstString = "name = 'txt1' AND index = " & Index
    txt1(Index) = objPRM.EingabeUmwandlung(txt1(Index), , False, True)
    If objPRM.EingabeFehler(txt1(Index)) = False Then
      gRS.Filter = getFilter()
      Call objPRM.SprungNeu("Rückwerts", Shift, txt1(Index).TabIndex, True)
    End If
    
  End Select

 '***Beginn
        Exit Sub
Fehler:
        Call FehlerErklärung("frmSP52850", "txt1_KeyDown")
 '***Ende

End Sub


Private Sub txt1_KeyPress(Index As Integer, KeyAscii As Integer)
 '***Beginn
On Error GoTo Fehler
 '***Ende
  Select Case KeyAscii
  Case vbKeyReturn, vbKeyEscape
    KeyAscii = 0
  End Select
  
  KeyAscii = objPRM.EingabePrüfung(KeyAscii)
  
  If txt1(Index).MaxLength = 0 Then
    'Prüfen, ob die neue Zeichenfolge, die zugelassene Länge übersteigt. (Numerischen Felder. Sonst gilt die MaxLength-Eigenschaft von clsPRM)
    Dim NeuText As String
    If txt1(Index).SelLength = 0 Then NeuText = Mid(txt1(Index), 1, txt1(Index).SelStart) & Chr(KeyAscii) & Mid(txt1(Index), txt1(Index).SelStart + 1)
    If KeyAscii <> 0 And KeyAscii <> 8 Then
      If Len(NeuText) > objPRM.EingabeLaenge(NeuText) Then
        'Die neue Zeichenfolge würde zu lang. Eingabe unterdrücken.
        Beep
        KeyAscii = 0
      End If
    End If
  End If

 '***Beginn
        Exit Sub
Fehler:
        Call FehlerErklärung("frmSP52850", "txt1_KeyPress")
 '***Ende

End Sub


Private Sub txt1_LostFocus(Index As Integer)
 '***Beginn
On Error GoTo Fehler
 '***Ende
  txt1(Index).ForeColor = vbActiveTitleBar
  txt1(Index).BackColor = vbWindowBackground  'Fensterhintergrund(weiß)
        
 '***Beginn
        Exit Sub
Fehler:
        Call FehlerErklärung("frmSP52850", "txt1_LostFocus")
 '***Ende

End Sub


Private Sub txt1_Validate(Index As Integer, Cancel As Boolean)
 '***Beginn
On Error GoTo Fehler
 '***Ende
  
  objPRM.FindFirstString = "name = 'txt1' AND index = " & Index
  txt1(Index) = objPRM.EingabeUmwandlung(txt1(Index), , False, True)
  If objPRM.EingabeFehler(txt1(Index)) Then
    Cancel = True
  Else
    gRS.Filter = getFilter()
  End If
  
 '***Beginn
        Exit Sub
Fehler:
        Call FehlerErklärung("frmSP52850", "txt1_Validate")
 '***Ende

End Sub



Public Sub SetStatus(Wert As Boolean)
  Dim bookmark As Variant
  
  If gRS.RecordCount > 0 Then
    bookmark = gRS.bookmark
    gRS.MoveFirst
    Do Until gRS.EOF
      gRS!Status = Wert
      gRS.Update
      gRS.MoveNext
    Loop
    gRS.bookmark = bookmark
  End If
  
End Sub

Public Sub UpdateTest()
  '*****
  'Die Routine darf nicht in das Programm eingebunden werden.
  'Das Speichern wird in LlPrint vorgenommen (Beim Drucken).
  '*****
  Dim Formular As String
  Dim i As Integer
  Dim Msg As Boolean
  Dim MsgBoxText As String
  Dim SperrText As String
  Dim DruckIni As Boolean
  Dim rs As DAO.Recordset
  Dim RS1 As DAO.Recordset
  Dim ZwSumme As Double
  Dim SQL As String
  
  Dim BelegDatum As Variant
  Dim BelegID As Long
  Dim Betrag As Double
  Dim Waehrung As String
  Dim Skonto As Single
  Dim SkontoTage As Integer
  Dim NettoTage As Integer
  Dim MwSt As Single
  Dim RechnNr As Long
 
  Dim Seite As Long
  Dim glRet As Long
  
  On Error GoTo Fehler:
  
  If gRS.RecordCount > 0 Then
    'GWS.BeginTrans
    
    gRS.MoveFirst
  
    Do Until gRS.EOF
      If gRS!Status = -1 Then
        Msg = False
        BelegID = gRS!BelegID
        Set rs = GDB.OpenRecordset("SELECT * FROM [2800_Haupt] WHERE BelegID = " & BelegID, dbOpenDynaset)
       
        If rs.RecordCount > 0 Then
          'Überprüfen, ob in der Zwischenzeit von einer andern Arbeitsstation der Beleg gedruckt wurde.
          If rs!Druck = 0 Then
            
            If rs!BelegNr = 0 Then
              'Neue Rechnung
              RechnNr = NummernKreis(NummernKreisWaehlen(gintPrivBelegArt + 8)) 'Prüfen, ob allgemainer Nummernkreis für Rechnungen benutzt werden soll.
              If RechnNr = -1 Then
                'Fehler beim Ziehen der Rechnungsnummer.
                Call MsgText(1, 338, 0, 0, 0)
                'Der Nummernkreis ist von einem anderen Benutzer kurzzeitig gesperrt. Bitte versuchen Sie den Vorgang nochmals auszuführen.
                MsgBox GsMsgText(0), vbInformation
                'GWS.Rollback
                'Exit Function

              End If
              'rs!BelegNr = RechnNr
            End If
            
            rs.Edit
            rs!Druck = 1
            rs!AendDat = Now
            rs!AendVon = GsUser
            If RechnNr > -1 Then rs!BelegNr = RechnNr
            rs.Update
          End If
        End If
      End If

NextRecord:
      
      gRS.MoveNext
    Loop
  gRS.MoveFirst
  
  End If

Exit Sub

Fehler:
        If IsUpdateError(Err.Number) Then
          If SperrText = "" Then
            SperrText = "Folgende Belege wurden nicht gedruckt da sie von anderen Benutzern bearbeitet waren:"
            SperrText = SperrText & vbCrLf & rs!BelegNr & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
          Else
            SperrText = SperrText & vbCrLf & rs!BelegNr & " - " & rs!Name1 & " Benutzer: " & rs!AendVon & " am " & rs!AendDat
          End If
          GoTo NextRecord
        Else
          gRS.MoveFirst
          Screen.MousePointer = 0
          If glRet <> 0 Then
            Call FehlerErklärung("SP52800B", "LLPrintListe LL-Fehler: " & glRet)
          Else
            Call FehlerErklärung("SP52800B", "LLPrintListe")
          End If
          'Beim Drucken ist ein fehler aufgeteten.
          'Die Änderungen in der DB müssen rückgängig gemacht werden.
          'evtl. RollBack
          glRet = LL_ERR_USER_ABORTED '-99
        
        End If

End Sub
