Private Sub TDBG1_HeadClick(ByVal ColIndex As Integer)
        '***Beginn
On Error GoTo Fehler
        '***Ende
    
    If gIntSortColIndex = ColIndex Then
      Select Case gstrSortOrder
      Case ""
        gstrSortOrder = "ASC"
        gRS.Sort = TDBG1.Columns(gIntSortColIndex).DataField & " " & gstrSortOrder
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicture = ImageList1.ListImages(1).Picture
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicturePosition = dbgFPRight
      Case "ASC"
        gstrSortOrder = "DESC"
        gRS.Sort = TDBG1.Columns(gIntSortColIndex).DataField & " " & gstrSortOrder
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicture = ImageList1.ListImages(2).Picture
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicturePosition = dbgFPRight
      Case "DESC"
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicturePosition = dbgFPTextOnly
        'gIntSortColIndex = 0
        gstrSortOrder = ""
        gRS.Sort = ""
      End Select
    Else
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicturePosition = dbgFPTextOnly
        gIntSortColIndex = ColIndex
        gstrSortOrder = "ASC"
        gRS.Sort = TDBG1.Columns(gIntSortColIndex).DataField & " " & gstrSortOrder
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicture = ImageList1.ListImages(1).Picture
        TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicturePosition = dbgFPRight
    End If
        
        
        '***Beginn
        Exit Sub
Fehler:
        If Err.Number = -2147217824 Then
          'Die Reihenfolge kann nicht angewendet werden.
          TDBG1.Columns(gIntSortColIndex).HeadingStyle.ForegroundPicturePosition = dbgFPTextOnly
          Resume Next
        Else
          Call FehlerErklärung("frmSP52850", "TDBG1_HeadClick")
        End If
        '***Ende

End Sub

