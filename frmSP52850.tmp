Public Function RsOhneVerbindung(QuellRs As ADODB.Recordset, Optional StatusFeld As String, Optional OhneInhalt As Boolean) As ADODB.Recordset
        '***Beginn
On Error GoTo Fehler
        '***Ende
  'Die Funktion erzeugt ein verbindungsloses Recordset mit der Struktur und Inhalt des QuellRs. (Wenn OhneInhalt = False)
  'StatusFeld wird eingefügt als numerisches Feld mit dem Standardwert = 0
  Dim i As Integer
  Dim bookmark As Variant
  Dim Text As String
  
  Set RsOhneVerbindung = New ADODB.Recordset
  For i = 0 To QuellRs.Fields.Count - 1
    If QuellRs(i).Name = "BelegNr" Then
      RsOhneVerbindung.Fields.Append QuellRs.Fields(i).Name, adVarWChar, 20, adFldIsNullable
    Else
      RsOhneVerbindung.Fields.Append QuellRs.Fields(i).Name, QuellRs.Fields(i).Type, QuellRs.Fields(i).DefinedSize, adFldIsNullable
    End If
  Next i
  
  If StatusFeld <> "" Then
    RsOhneVerbindung.Fields.Append StatusFeld, adSmallInt
  End If
  
  RsOhneVerbindung.Open
  
  If OhneInhalt Then Exit Function
  
  If QuellRs.RecordCount > 0 Then
    bookmark = QuellRs.bookmark
    QuellRs.MoveFirst
  End If
  
  Do Until QuellRs.EOF
    RsOhneVerbindung.AddNew
    For i = 0 To QuellRs.Fields.Count - 1
      If QuellRs(i).Type = adVarWChar Then
        RsOhneVerbindung.Fields(i).Value = "" & QuellRs.Fields(i).Value
      Else
        RsOhneVerbindung.Fields(i).Value = QuellRs.Fields(i).Value
      End If
      If QuellRs(i).Name = "BelegNr" Then
        If RsOhneVerbindung.Fields(i).Value = 0 Then RsOhneVerbindung.Fields(i).Value = "neu"
      End If
    Next i
    If StatusFeld <> "" Then
      RsOhneVerbindung.Fields(i).Value = -1
    End If
    
    RsOhneVerbindung.Update
    QuellRs.MoveNext
  Loop
  
  If QuellRs.RecordCount > 0 Then
    RsOhneVerbindung.MoveFirst
    QuellRs.bookmark = bookmark
  End If
  
        '***Beginn
        Exit Function
Fehler:
        Call FehlerErklärung("SP52800B", "RsOhneVerbindung")
        '***Ende
End Function







